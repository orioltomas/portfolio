{% extends 'layout.html.twig' %}

{% block content %}

    <div class="vincle-studio text-center">
        <div class="portfolio__project-back-wrapper">
            <a class="portfolio__project-back" href="{{ path('portfolio') }}">< {{ 'portfolio.project.back' | trans | upper }}</a>
        </div>
        <h1 class="pb-4 pb-lg-5">VINCLE STUDIO</h1>
        <div class="container">
            <div class="col-12">
                <div class="vincle-studio__description text-left pb-5">
                    <h5 class="font-weight-lighter pb-3">{{ 'portfolio.project.about' | trans | upper }}</h5>
                    <div class="vincle-studio__content pb-3">
                        {{ 'portfolio.project.vincle-studio.description' | trans | raw }}
                    </div>
                    <h5 class="font-weight-lighter pt-3 pb-3">{{ 'portfolio.project.details' | trans | upper }}</h5>
                    <div>{{ 'portfolio.project.vincle-studio.development' | trans }}</div>
                    <div>{{ 'portfolio.project.vincle-studio.date' | trans | upper }}</div>
                    <div class="vincle-studio__url">
                        <a href="http://vinclestudio.com/" target="_blank">WWW.VINCLESTUDIO.COM</a>
                    </div>
                </div>
                <div class="vincle-studio__project pt-3">
                    <img class="vincle-studio__image" src="{{ asset('build/images/vincle-studio/vinclestudio1.png') }}">
                    <img class="vincle-studio__image" src="{{ asset('build/images/vincle-studio/vinclestudio2.png') }}">
                    <img class="vincle-studio__image" src="{{ asset('build/images/vincle-studio/vinclestudio3.png') }}">
                    <img class="vincle-studio__image" src="{{ asset('build/images/vincle-studio/vinclestudio4.png') }}">
                    <img class="vincle-studio__image" src="{{ asset('build/images/vincle-studio/vinclestudio5.png') }}">
                    <img class="vincle-studio__image" src="{{ asset('build/images/vincle-studio/vinclestudio6.png') }}">
                </div>
            </div>
        </div>
    </div>

{% endblock %}